{
  "name": "protolabo-jsutils",
  "version": "0.8.3",
  "description": "Javascript utility and helper methods",
  "private": true,
  "browser": "dist/jslib.min.js",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "node scripts/build.js && npm run build:code",
    "postbuild": "npm run build:docs",
    "build:code": "rollup -c",
    "build:docs": "documentation build src/utils/index.js -f html -o docs/utils",
    "bundle:iife": "rollup src/index.js --file bundle.iife.js --format iife --name jsLabo",
    "bundle:umd": "rollup src/index.js --file bundle.umd.js --format umd --name jsLabo",
    "test": "mocha --require @babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/protolabo/jslib.git"
  },
  "keywords": [
    "js",
    "utils"
  ],
  "author": "Louis-Edouard LAFONTANT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/protolabo/jslib/issues"
  },
  "homepage": "https://github.com/protolabo/jslib#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.2.0",
    "clean-css": "^4.2.1",
    "coffeescript": "^2.3.2",
    "jsdom": "^15.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-uglify": "^6.0.1"
  },
  "dependencies": {
    "cross-env": "^5.2.0"
  }
}
